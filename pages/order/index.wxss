.myflex {
  display: block;
  color: #fff;
  padding: 10rpx 10rpx;
  font-size: 28rpx;
  background: -webkit-linear-gradient(bottom right, #fff 60%, #53bc64 100%);
  background: linear-gradient(to top left, #19be6b 40%, #19be6b 100%);
  background-position: top center;
  background-size: 750rpx 100%;
}
.goto-pic {
  width: 140rpx;
  height: 30rpx;
  margin-top: -10rpx;
  margin-bottom: 10rpx;
}
.bus-top {
  line-height: 2;
  padding: 20rpx 30rpx;
  text-align: center;
}
.start-time,
.end-time {
  font-size: 42rpx;
  font-weight: 600;
}
.start-site {
  font-size: 28rpx;
}
.mid-flex {
  line-height: 1.2;
  padding: 20rpx 20rpx;
}
.price-container {
  margin-top: 20rpx;
}
.mid-container {
  display: block;
  width: 96%;
  margin: 20rpx 2%;
  padding: 20rpx 0;
  background: #fff;
}
.flex-list {
  display: block;
  width: 100%;
}
.r-flex {
  position: relative;
  margin: 20rpx 10rpx 0 10rpx;
  width: 100%;
  font-size: 26rpx;
}
.bottom-bar {
  display: block;
  width: 100%;
  position: fixed;
  z-index: 999;
  left: 0;
  bottom: 0;
  height: 120rpx;
  background: #fff;
  text-align: center;
  line-height: 120rpx;
  font-size: 32rpx;
}
.submit-button {
  background: #53bc64;
  color: #fff;
  font-size: 32rpx;
}
.myprice1 {
  color: #ff4422;
}
.popup__content {
  width: 100%;
  height: 500rpx;
  max-height: 700rpx;
  line-height: 200rpx;
  text-align: center;
  background: #FFFFFF;
}
.input__form {
  font-size: 12rpx !important;
}
.wrap {
  display: block;
  box-sizing: border-box;
  height: 100rpx;
  width: 97%;
  padding: 0rpx 2%;
  border: 1rpx solid #e3e3e3;
  margin: 2% 1.5% 0 1.5%;
}
.item {
  box-sizing: border-box;
  font-size: 28rpx;
  line-height: 100rpx;
}
.side {
  width: 200rpx;
  flex-grow: 0;
  text-align: left;
  padding-left: 15rpx;
}
.main {
  flex-grow: 1;
  text-align: right;
  color: #999999;
  padding-right: 20rpx;
}
.myinput {
  font-size: 26rpx !important;
}
